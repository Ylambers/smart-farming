<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Admin panel{% endblock %}</title>

    {% stylesheets
    'bundles/css/default/bootstrap.css' filter='cssrewrite'
    'bundles/css/default/fos.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>

{% include '::topnav.html.twig' %}

{% block content %}

    {% if app.request.hasPreviousSession %}
        {% for type, messages in app.session.flashbag.all() %}
            {% for message in messages %}
                <div class="flash-{{ type }}">
                    {{ message }}
                </div>
            {% endfor %}
        {% endfor %}
    {% endif %}

    <div>
        {% block fos_user_content %}
        {% endblock fos_user_content %}
    </div>
{% endblock %}

{% block javascripts %}
    {% javascripts
    'bundles/js/jquery-3.1.1.js'
    'bundles/js/bootstrap.js'
    'bundles/js/admin/index.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {#CDN LIBARAYS#}
    <script src="//cdn.rawgit.com/noelboss/featherlight/1.6.1/release/featherlight.min.js" type="text/javascript" charset="utf-8"></script>
    {#END CDN#}
{% endblock %}
