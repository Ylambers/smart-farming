{% extends '::base.html.twig' %}

{% block content %}
<div class="container top-container">
    <h1>Companymemberfunctions list</h1>

    <table class="table  table-striped table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for companyMemberFunction in companyMemberFunctions %}
            <tr>
                <td><a href="{{ path('user_companymemberfunction_show', { 'id': companyMemberFunction.id }) }}">{{ companyMemberFunction.id }}</a></td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('user_companymemberfunction_show', { 'id': companyMemberFunction.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('user_companymemberfunction_edit', { 'id': companyMemberFunction.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('user_companymemberfunction_new') }}">Create a new companyMemberFunction</a>
        </li>
    </ul>
</div>
{% endblock %}
