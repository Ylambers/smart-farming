{% extends '::base.html.twig' %}

{% block content %}
<div class="container top-container">
    <h1>Companyfunction</h1>

    <table class="table table-striped table-hover">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ companyFunction.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ companyFunction.title }}</td>
            </tr>
        </tbody>
    </table>

    <ul>

            <a class="btn btn-default" href="{{ path('companyfunction_index') }}">Terug naar overzicht</a>
            <a class="btn btn-default" href="{{ path('companyfunction_edit', { 'id': companyFunction.id }) }}">Aanpassen</a>
            {{ form_start(delete_form) }}
                <input class="btn btn-default" type="submit" value="Verwijderen">
            {{ form_end(delete_form) }}
    </ul>
</div>
{% endblock %}
